  "version": "0.2.0",
  "configurations": [
    {
      "name": "Test",
      "request": "launch",
      "runtimeArgs": [
        "test"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node",
      "env": {
        "DB_HOST":"localhost",
        "DB_USER":"root",
        "DB_PASSWORD":"Hello@123",
        "DATABASE":"assignments"
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\main.js",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "env": {
        "PORT": "5000",
        "DB_HOST":"localhost",
        "DB_USER":"root",
        "DB_PASSWORD":"Hello@123",
        "DATABASE":"assignments"
      }
    }
  ]
}
